{
    "version": 2,
    "builds": [
        {
            "src": "dash/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12.3"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/",
            "dest": "/static/"
        },
        {
            "src": "/(.*)",
            "dest": "dash/asgi.py"
        }
    ]

}